            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckStudentRecord"
            android:screenOrientation="landscape">

        </activity>
        <activity android:name=".CreateStudentRecord"
            android:screenOrientation="landscape">

        </activity>
        <activity android:name=".ChooseStudentRecord"
            android:screenOrientation="landscape">

        </activity>
        <activity android:name=".ChooseModule"
            android:screenOrientation="landscape">

        </activity>
        <activity android:name=".PSCQuestions"
            android:screenOrientation="landscape">

        </activity>
    </application>

</manifest>
